{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "type": {
            "type": "string",
            "const": "push_captured_image"
        },
        "bound_to": {
            "type": "string",
            "const": "client"
        },
        "shape": {
            "type": "object",
            "properties": {
                "width": {
                    "type": "integer"
                },
                "height": {
                    "type": "integer"
                }
            },
            "required": [
                "width",
                "height"
            ]
        },
        "data": {
            "type": "string"
        }
    },
    "required": [
        "type",
        "bound_to",
        "shape",
        "data"
    ]
}
